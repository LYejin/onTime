<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kosa.scheduleManagement.global.dao.Project_EmpDao">

	<insert id="insertProject_Emp" parameterType="com.kosa.scheduleManagement.global.dto.Project_Sub">
		<foreach item="emp" collection="emplist" separator=";">
			INSERT INTO Project_Emp (project_num, user_id)
			VALUES (#{project.project_num}, #{emp.user_id})
		</foreach>
	</insert>
</mapper>
	